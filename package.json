{
  "name": "@vitrozsival/nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production next build",
    "cz": "cz",
    "dev": "next dev",
    "eslint": "eslint --cache --ext js,ts,tsx .",
    "eslint:fix": "pnpm run eslint --fix",
    "fix": "run-s ts eslint:fix stylelint:fix prettier:fix",
    "prepare": "is-ci || husky install",
    "prettier": "prettier --check \"./**/*.{json,md,yml}\"",
    "prettier:fix": "pnpm run prettier --write",
    "stylelint": "stylelint --cache \"./**/*.{ts,tsx}\"",
    "stylelint:fix": "pnpm run stylelint --fix",
    "qa": "run-s ts eslint stylelint prettier",
    "start": "NODE_ENV=production next start",
    "ts": "tsc --project tsconfig.json"
  },
  "dependencies": {
    "@emotion/cache": "11.10.5",
    "@emotion/react": "11.10.6",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.6",
    "next": "13.1.6",
    "nprogress": "0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@emotion/babel-plugin": "11.10.6",
    "@emotion/eslint-plugin": "11.10.0",
    "@types/node": "18.14.0",
    "@types/nprogress": "0.2.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "commitizen": "4.3.0",
    "eslint": "8.34.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-formatjs": "4.7.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unicorn": "45.0.2",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "lint-staged": "13.1.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "postcss-styled-syntax": "0.3.3",
    "prettier": "2.8.4",
    "prettier-plugin-package": "1.3.0",
    "stylelint": "15.1.0",
    "stylelint-config-recommended": "10.0.1",
    "stylelint-order": "6.0.2",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "packageManager": "pnpm@7.27.1"
}
